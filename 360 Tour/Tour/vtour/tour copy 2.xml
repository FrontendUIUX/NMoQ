<plugin name="custom_controls_container"
		id="custom_controls_container"
        type="container"
        align="bottom"
        x="0"
        y="10"
        width="300"
        height="50"
        visible="false"
        keep="true">

    <!-- Zoom In Plugin -->
    <plugin name="custom_zoom_in"
            url="skin/zoomIn.png"
            keep="true"
            align="left"
            x="0"
            y="0"
            autosize="true"
            scale="1.0"
            handcursor="true"
            ondown="set(fov_moveforce,-1);"     
            onup="set(fov_moveforce,0);"
            bgcolor="0x000000"
            border="true"
            bordercolor="0xFFFFFF" />

    <!-- Zoom Out Plugin -->
    <plugin name="custom_zoom_out"
            url="skin/zoomOut.png"
            keep="true"
            align="left"
            x="40"
            y="0"
            autosize="true"
            scale="1.0"
            handcursor="true"
            ondown="set(fov_moveforce,+1);"     
            onup="set(fov_moveforce,0);" />

    <!-- Movement Controls -->
    <plugin name="moving_controls"
            url="skin/movements.png"
            keep="true"
            align="left"
            x="80"
            y="0"
            autosize="true"
            scale="1.0"
            handcursor="true"
            onclick="toggle_plugins();" />

    <!-- Hide Toolbar Button -->
    <plugin name="hide_toolbar"
            url="skin/view.png"
            keep="true"
            align="left"
            x="120"
            y="0"
            autosize="true"
            scale="1.0"
            handcursor="true"
            onclick="toggle_toolbar_visibility();" />

    <!-- Full Screen Button -->
    <plugin name="full_screen"
            url="skin/fullscreen.png"
            keep="true"
            align="left"
            x="160"
            y="0"
            autosize="true"
            scale="1.0"
            handcursor="true"
            onclick="switch(fullscreen);" 
            devices="fullscreensupport" />

    <!-- Sound Button -->
    <plugin name="enable_sound"
            url="skin/sound.png"
            keep="true"
            align="left"
            x="200"
            y="0"
            autosize="true"
            scale="1.0"
            handcursor="true"
            ondown="set(fov_moveforce,+1);"     
            onup="set(fov_moveforce,0);" />

    <!-- How To Use Tooltip -->
    <plugin name="how_to_use"
            url="skin/tooltip.png"
            keep="true"
            align="left"
            x="240"
            y="0"
            autosize="true"
            scale="1.0"
            handcursor="true"
            ondown="set(fov_moveforce,+1);"     
            onup="set(fov_moveforce,0);" />

</plugin>
